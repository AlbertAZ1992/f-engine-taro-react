{
  "name": "f-engine-taro-react",
  "version": "1.0.0-beta",
  "description": "FEngine for Taro. Supporting Taro v4 and F2 v5",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js"
    },
    "./engine": {
      "types": "./dist/engine/index.d.ts",
      "require": "./dist/engine/index.js",
      "import": "./dist/engine/index.esm.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "require": "./dist/utils/index.js",
      "import": "./dist/utils/index.esm.js"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "keywords": [
    "taro",
    "taro components",
    "taro react",
    "antv",
    "f2",
    "f-engine",
    "f2 v5",
    "chart",
    "charts",
    "mobile",
    "visualization",
    "react"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "https://github.com/AlbertAZ1992",
  "homepage": "https://github.com/AlbertAZ1992/f-engine-taro-react#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlbertAZ1992/f-engine-taro-react"
  },
  "scripts": {
    "build": "rimraf ./dist && rollup -c",
    "clean": "rimraf ./dist",
    "dev": "rollup -c -w",
    "lint": "npx biome format --write",
    "test": "jest --config jest.config.js"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "peerDependencies": {
    "@antv/f2": "^5.9.0",
    "@tarojs/components": "4.1.5",
    "@tarojs/runtime": "4.1.5",
    "@tarojs/shared": "4.1.5",
    "@tarojs/taro": "4.1.5",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@antv/f-engine": "^1.8.0"
  },
  "devDependencies": {
    "@antv/f2": "^5.9.0",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tarojs/components": "4.1.5",
    "@tarojs/plugin-platform-h5": "^4.1.9",
    "@tarojs/plugin-platform-weapp": "^4.1.9",
    "@tarojs/runtime": "4.1.5",
    "@tarojs/shared": "4.1.5",
    "@tarojs/taro": "^4.1.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.2.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.16.4",
    "rollup-plugin-node-externals": "^7.1.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.7.3"
  }
}
